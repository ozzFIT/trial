<div id="map"></div>
<style>
    #map {
        height: 500px;
        width: 400px;
    }
</style>
<br />
<div class="row">
    <div id="map"></div>
    <script>
        var map = L.map('map')
            .setView([-37.8748928, 145.0442752], 15);

        L.tileLayer('http://{s}.tile.osm.org/{z}/{x}/{y}.png', {
            attribution: '&copy; <a href="http://osm.org/copyright">OpenStreetMap</a> contributors'
        }).addTo(map);

            //var marker = new google.maps.Marker({ position: { lat: -37.8748928, lng: 145.0442752 }, map: map });
    </script>

